{
  "name": "mdx",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "section"
      }
    },
    "section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "standalone_jsx"
        },
        {
          "type": "SYMBOL",
          "name": "markdown"
        },
        {
          "type": "CHOICE",
          "members": []
        }
      ]
    },
    "standalone_jsx": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "import_statement"
          }
        ]
      }
    },
    "import_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "import_start"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "identifier"
                  },
                  {
                    "type": "STRING",
                    "value": "{"
                  },
                  {
                    "type": "STRING",
                    "value": "}"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "semicolon"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "import_start": {
      "type": "PATTERN",
      "value": "import"
    },
    "export_start": {
      "type": "PATTERN",
      "value": "export"
    },
    "statement_body": {
      "type": "PATTERN",
      "value": "[^;]+"
    },
    "semicolon": {
      "type": "PATTERN",
      "value": ";"
    },
    "markdown": {
      "type": "PATTERN",
      "value": "[a-z\\ ]+"
    },
    "word": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z_]+[a-z_0-9]*"
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "double_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "single_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "backtick_quoted_string"
        }
      ]
    },
    "double_quoted_string": {
      "type": "PATTERN",
      "value": "\".*\""
    },
    "single_quoted_string": {
      "type": "PATTERN",
      "value": "'.*'"
    },
    "backtick_quoted_string": {
      "type": "PATTERN",
      "value": "`.*`"
    },
    "_newline_token": {
      "type": "PATTERN",
      "value": "\\n|\\r\\n?"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

