{
  "name": "mdx",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "jsx_section"
            },
            {
              "type": "SYMBOL",
              "name": "markdown_section"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "newline_token"
              },
              {
                "type": "SYMBOL",
                "name": "newline_token"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "jsx_section"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "markdown_section"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "jsx_section": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "import_statement"
              },
              {
                "type": "SYMBOL",
                "name": "html_component"
              },
              {
                "type": "SYMBOL",
                "name": "jsx_component"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "newline_token"
          }
        ]
      }
    },
    "markdown_section": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "interpolation"
              },
              {
                "type": "SYMBOL",
                "name": "markdown_text"
              }
            ]
          }
        ]
      }
    },
    "interpolation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "interpolation_start"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        },
        {
          "type": "PATTERN",
          "value": "\\}"
        }
      ]
    },
    "markdown_text": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "\\S+\\s+"
          }
        }
      }
    },
    "import_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "import_start"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "ALIAS",
                          "content": {
                            "type": "PATTERN",
                            "value": "\\{"
                          },
                          "named": true,
                          "value": "left_brace"
                        },
                        {
                          "type": "REPEAT",
                          "content": {
                            "type": "CHOICE",
                            "members": [
                              {
                                "type": "SYMBOL",
                                "name": "identifier"
                              },
                              {
                                "type": "STRING",
                                "value": ","
                              }
                            ]
                          }
                        },
                        {
                          "type": "ALIAS",
                          "content": {
                            "type": "PATTERN",
                            "value": "\\}"
                          },
                          "named": true,
                          "value": "right_brace"
                        }
                      ]
                    },
                    {
                      "type": "SYMBOL",
                      "name": "identifier"
                    }
                  ]
                },
                {
                  "type": "STRING",
                  "value": "from"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "semicolon"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "html_component": {
      "type": "CHOICE",
      "members": []
    },
    "jsx_component": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^\\\\]<"
        },
        {
          "type": "PATTERN",
          "value": ".+"
        },
        {
          "type": "PATTERN",
          "value": "[^\\\\]\\/>"
        }
      ]
    },
    "semicolon": {
      "type": "PATTERN",
      "value": ";"
    },
    "word": {
      "type": "PATTERN",
      "value": "[/S]+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-z_]+[A-Za-z_0-9]*"
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "double_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "single_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "backtick_quoted_string"
        }
      ]
    },
    "double_quoted_string": {
      "type": "PATTERN",
      "value": "\".*\""
    },
    "single_quoted_string": {
      "type": "PATTERN",
      "value": "'.*'"
    },
    "backtick_quoted_string": {
      "type": "PATTERN",
      "value": "`.*`"
    },
    "newline_token": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\n|\\r\\n?"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[^/S/r/n]+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "import_start"
    },
    {
      "type": "SYMBOL",
      "name": "interpolation_start"
    },
    {
      "type": "SYMBOL",
      "name": "markdown"
    },
    {
      "type": "SYMBOL",
      "name": "eof"
    }
  ],
  "inline": [],
  "supertypes": []
}
