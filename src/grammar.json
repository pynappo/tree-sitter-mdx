{
  "name": "mdx",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "jsx_section"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "markdown_section"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "newline_token"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "newline_token"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "newline_token"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "jsx_section"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "markdown_section"
              }
            ]
          }
        }
      ]
    },
    "jsx_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "import_statement"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "newline_token"
        }
      ]
    },
    "markdown_section": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\\r\\n]+"
              }
            ]
          }
        ]
      }
    },
    "import_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "import_start"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "identifier"
              },
              {
                "type": "STRING",
                "value": "{"
              },
              {
                "type": "STRING",
                "value": "}"
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "SYMBOL",
                    "name": "newline_token"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "semicolon"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "import_start": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "import"
      }
    },
    "export_start": {
      "type": "PATTERN",
      "value": "export"
    },
    "statement_body": {
      "type": "PATTERN",
      "value": "[^;]+"
    },
    "semicolon": {
      "type": "PATTERN",
      "value": ";"
    },
    "word": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-z_]+[A-Za-z_0-9]*"
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "double_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "single_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "backtick_quoted_string"
        }
      ]
    },
    "double_quoted_string": {
      "type": "PATTERN",
      "value": "\".*\""
    },
    "single_quoted_string": {
      "type": "PATTERN",
      "value": "'.*'"
    },
    "backtick_quoted_string": {
      "type": "PATTERN",
      "value": "`.*`"
    },
    "newline_token": {
      "type": "PATTERN",
      "value": "\\n|\\r\\n?"
    },
    "_horizontal_whitespace": {
      "type": "PATTERN",
      "value": "[^\\S\\r\\n]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

