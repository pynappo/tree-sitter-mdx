{
  "name": "mdx",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "jsx_section"
                },
                {
                  "type": "SYMBOL",
                  "name": "markdown_section"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "end_of_line"
              },
              {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "end_of_line"
                }
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "jsx_section"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "markdown_section"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "jsx_section": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "import_statement"
              },
              {
                "type": "SYMBOL",
                "name": "html_component"
              },
              {
                "type": "SYMBOL",
                "name": "jsx_component"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "end_of_line"
          }
        ]
      }
    },
    "markdown_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "escaped_slash"
              },
              {
                "type": "SYMBOL",
                "name": "interpolation"
              },
              {
                "type": "SYMBOL",
                "name": "markdown_word"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "end_of_line"
        }
      ]
    },
    "interpolation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "interpolation_start"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "interpolation_content"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\}"
          },
          "named": true,
          "value": "interpolation_end"
        }
      ]
    },
    "markdown_word": {
      "type": "PATTERN",
      "value": "[\\S][^\\{\\s\\r\\n]+"
    },
    "escaped_slash": {
      "type": "PATTERN",
      "value": "\\\\\\{"
    },
    "import_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "import_start"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "ALIAS",
                          "content": {
                            "type": "PATTERN",
                            "value": "\\{"
                          },
                          "named": true,
                          "value": "left_brace"
                        },
                        {
                          "type": "REPEAT",
                          "content": {
                            "type": "CHOICE",
                            "members": [
                              {
                                "type": "SYMBOL",
                                "name": "jsx_identifier"
                              },
                              {
                                "type": "STRING",
                                "value": ","
                              }
                            ]
                          }
                        },
                        {
                          "type": "ALIAS",
                          "content": {
                            "type": "PATTERN",
                            "value": "\\}"
                          },
                          "named": true,
                          "value": "right_brace"
                        }
                      ]
                    },
                    {
                      "type": "SYMBOL",
                      "name": "jsx_identifier"
                    }
                  ]
                },
                {
                  "type": "PATTERN",
                  "value": "from"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "jsx_string"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": ";"
          },
          "named": true,
          "value": "semicolon"
        }
      ]
    },
    "html_component": {
      "type": "CHOICE",
      "members": []
    },
    "jsx_component": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^\\\\]<"
        },
        {
          "type": "PATTERN",
          "value": ".+"
        },
        {
          "type": "PATTERN",
          "value": "[^\\\\]\\/>"
        }
      ]
    },
    "jsx_identifier": {
      "type": "PATTERN",
      "value": "[$a-zA-z_]+[A-Za-z_0-9$]*"
    },
    "jsx_string": {
      "type": "FIELD",
      "name": "type",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "double_quoted_string"
          },
          {
            "type": "SYMBOL",
            "name": "single_quoted_string"
          },
          {
            "type": "SYMBOL",
            "name": "backtick_quoted_string"
          }
        ]
      }
    },
    "double_quoted_string": {
      "type": "PATTERN",
      "value": "\".*\""
    },
    "single_quoted_string": {
      "type": "PATTERN",
      "value": "'.*'"
    },
    "backtick_quoted_string": {
      "type": "PATTERN",
      "value": "`.*`"
    },
    "end_of_line": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\n|\\r\\n"
      }
    },
    "_word": {
      "type": "PATTERN",
      "value": "[/S]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[^/S/r/n]+"
    }
  ],
  "conflicts": [],
  "precedences": [
    [
      {
        "type": "SYMBOL",
        "name": "jsx_section"
      },
      {
        "type": "SYMBOL",
        "name": "markdown_section"
      }
    ],
    [
      {
        "type": "SYMBOL",
        "name": "escaped_slash"
      },
      {
        "type": "SYMBOL",
        "name": "interpolation"
      },
      {
        "type": "SYMBOL",
        "name": "markdown_word"
      }
    ],
    [
      {
        "type": "SYMBOL",
        "name": "end_of_line"
      },
      {
        "type": "SYMBOL",
        "name": "interpolation_start"
      }
    ]
  ],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "import_start"
    },
    {
      "type": "SYMBOL",
      "name": "interpolation_start"
    },
    {
      "type": "SYMBOL",
      "name": "interpolation_content"
    },
    {
      "type": "SYMBOL",
      "name": "eof"
    },
    {
      "type": "SYMBOL",
      "name": "error"
    }
  ],
  "inline": [],
  "supertypes": []
}
